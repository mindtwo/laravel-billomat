{
    "name": "mindtwo/laravel-billomat",
    "description": "Using the Billomat API with Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "5.6.*|5.7.*|5.8.*|6.0.*",
        "illuminate/config": "5.6.*|5.7.*|5.8.*|6.0.*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.0.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Mindtwo\\LaravelBillomat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mindtwo\\LaravelBillomat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindtwo\\LaravelBillomat\\Providers\\LaravelBillomatServiceProvider"
            ],
            "aliases": {
                "BillomatClient": "Mindtwo\\LaravelBillomat\\Facades\\Client"
            }
        }
    }
}

